<%-include("header") -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>
    <div class="box">
      <% for(i=0; i< todoItems.length; i++) { %>
      <div class="item"> 
        <input type="checkbox">     
        <p><%= todoItems[i] %></p>
      </div>
      <% } %>      
      <form class="item" action="/" method="post">
        <input type="text" name="todoItem" placeholder="Add more list" autocomplete="off" required />
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
    </div>
   
  <%-include("footer") -%>